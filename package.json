{
  "name": "fulll-fleet-management",
  "version": "0.1.0",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration && npm run test:feature",
    "test:unit": "jest --config jest.unit.js",
    "test:integration": "jest --config jest.integration.js",
    "test:feature": "TSX_TSCONFIG_PATH=tsconfig.feature.json cucumber-js --parallel 10",
    "coverage:unit": "jest --config jest.unit.js --coverage --coverageDirectory coverage/unit",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "circular": "madge --circular --extensions ts src"
  },
  "bin": {
    "fleet": "./src/infra/cli/index.ts"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "commander": "^14.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@cucumber/cucumber": "^12.2.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.15",
    "madge": "^8.0.0",
    "ts-jest": "^29.4.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20"
  }
}
